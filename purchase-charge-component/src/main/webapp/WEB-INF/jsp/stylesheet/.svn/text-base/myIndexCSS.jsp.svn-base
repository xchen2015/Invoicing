<%@ taglib prefix='c' uri='http://java.sun.com/jsp/jstl/core'%>
<% response.setContentType ("text/css"); %>

@charset "utf-8";
	
/*bd*/
.col-main {
	float: left;
	width: 100%;
}

.col-extra {
	float: left;
	width: 290px;
	margin-left: -290px;
}

/*col-main*/
.main-wrap {
	margin: 0 300px 0 18px;
	border: 1px solid #d4d4d4;
	background: #FFF;
	box-shadow: 0 1px 1px rgba(0, 0, 0, 0.11);
}

.m-top {
	padding: 8px;
	font-size: 14px;
}

.m-top table {
	table-layout: fixed;
	color: #FFF;
}

.m-top table td {
	padding: 3px;
}

.m-top table td div span {
	display: block;
	margin-top: 3px;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
}

.m-top table td div b {
	font-size: 16px;
	font-weight: normal;
	color: #F74C23;
}

.m-top li {
	display: table-cell;
	width: 19.99%;
	float: left;
}

.m-top li b {
	display: block;
}

.m-top li em {
	display: inline-block;
	white-space: nowrap;
	color: #F00;
}
