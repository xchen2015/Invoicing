    <!-- another one page navigation service for sub navigation accessing it
		will be refferenced by a servlet controller in PcApp-servlet.xml -->
	<bean id="subNavigationService"
		class="com.pinfly.purchasecharge.app.nav.SimpleNavigationService">
		<property name="configuration" ref="subNavigationConfigItems" />
	</bean>
	<!-- util list is configured as list of NavigationItem which configure the
		layout for components. this util list instance will also be referenced by
		popup navigation component. don't change its id. To configure this NavigationItems,
		just follow the rule of Spring bean navigationService. And note the path
		is different from the one in above. and the template is the name with prefix
		of 'sub'. -->
	<util:list id="subNavigationConfigItems">
		<bean class="com.pinfly.purchasecharge.app.nav.NavigationItem">
			<property name="path" value="/page2" />
			<property name="titleCode" value="appSetting.changePassword" />
			<property name="type" value="PAGE" />
			<property name="template" value="sublayout" />
			<property name="regionWidgetMap">
				<map>
					<entry key="main">
						<list>
							<ref bean="passwordManagementWidget" />
						</list>
					</entry>
				</map>
			</property>
		</bean>
	</util:list>
	
