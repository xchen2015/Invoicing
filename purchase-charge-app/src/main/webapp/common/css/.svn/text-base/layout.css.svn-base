a.companyName {
	color: #333;
	cursor: pointer;
	font: bold 27px/1.5 'Microsoft Yahei';
	text-shadow: 0 0 1px rgba(0, 0, 0, 0.2);
	max-width: 360px;
	/*float: left;*/
	overflow: hidden;
	display: block;
	white-space: nowrap;
	text-overflow: ellipsis;
	margin-right: 10px;
}

a.companyName:hover {
	text-decoration: none;
}