}

#left .component {
	margin-left: 7px;
	margin-right: 7px;
}

#right {
	width: 49.9%;
	display: inline;
	float: left;
}

#right .component {
	margin-right: 7px;
}

#bottom {
	width: 99.8%;
	display: inline;
