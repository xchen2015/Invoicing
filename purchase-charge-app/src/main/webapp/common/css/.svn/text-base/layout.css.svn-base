@charset "UTF-8";

@import url("icons.css");
/*
 * Defines CSS styles used for laying out an application page.
 *
 */
html,body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,p,blockquote,th,td
	{
	margin: 0;
	padding: 0;
}

img,body,html {
	border: 0;
}

address,caption,cite,code,dfn,em,strong,th,var {
	font-style: normal;
	font-weight: normal;
}

ol,ul {
	list-style: none;
}

caption,th {
	text-align: left;
}

h1,h2,h3,h4,h5,h6 {
	font-size: 100%;
}

q:before,q:after {
	content: '';
}

body {
	font: 80%/1.4 "Helvetica Neue",Helvetica,Arial,"Hiragino Sans GB","Hiragino Sans GB W3","Microsoft YaHei UI","Microsoft YaHei","WenQuanYi Micro Hei",sans-serif;
	background-color: #fefefe;
	color: #000000;
	min-width: 1020px;
	overflow-y: hidden;
}

h1,h3,h5 {
	color: #002733;
	font-family: Arial, Helvetica, sans-serif;
}

h2,h4,h6 {
	color: #330000;
	font-family: Arial, Helvetica, sans-serif;
}

a img {
	border: none;
}

/*Cxx commented*/
a:link { /*color: #003399;*/
	text-decoration: none;
}

a:visited, a:focus, a:active {
	text-decoration: none;
	/*color: #003399;*/
}

a:hover {
	text-decoration: underline;
}

.loadingProgress {
	width: 100%;
	min-height: 450px;
	height: auto !important;
	height: 450px;
}

.loadingProgress img {
	margin-left: 50%;
	margin-top: 100px;
}

.loginBox {
	margin: 10px 10px 10px 0px;
	border: 1px solid #c4d8eb;
	background-color: #ffffff;
	padding: 3px;
}

.loginBox div {
	background-color: #e5eefa;
	padding: 1px;
}

.loginBox h3 {
	margin-top: 5px;
	margin-bottom: 5px;
}

.loginBox h3,.loginBox p {
	padding-left: 10px;
	padding-right: 10px;
}

.loginBox label {
	color: #000033;
}

.header {
	background: #a6c2d9 url(../images/bg.png) 0px -182px repeat-x;
	border-bottom: 1px solid #ffffff;
	position: relative;
}

.header img {
	vertical-align: middle;
}

.logo {
	padding: 10px 10px 15px 10px;
}

.userMenu {
	font-size: .85em;
	float: right;
	padding-top: 0px;
	margin-right: 3px;
}

.userMenu button {
	font-size: 12px;
	padding: 4px 5px 4px 5px;
	border: 1px;
	background: url(../images/bg.png) 0 -421px repeat-x;
}

.userMenu button:hover {
	background: url(../images/bg.png) 0 -292px repeat-x;
}

.principalName {
	color: #003399;
}

#content { /*min-height: 500px;
	height: auto !important;*/
	height: 500px;
	padding-top: 7px;
	overflow-y: auto;
	overflow-x: hidden;
	clear: both;
}

#subContent {
	min-height: 400px;
	height: auto !important;
	height: 400px;
	padding: 3px;
}

.component {
	margin-bottom: 7px;
}

.footer {
	padding: 10px;
	background: #a6c2d9 url(../images/bg.png) 0px 0px repeat-x;
	border-top: 1px solid #ffffff;
	text-align: center;
	position: fixed;
	left: 0px;
	right: 0px;
	bottom: 0px;
	clear: both;
	min-height: 1.2em;
	font-size: .9em;
}

.footer ul {
	list-style: none;
}

.footer ul li {
	background: url(../images/icons.png) no-repeat scroll -555px -76px
		transparent;
	display: inline-block;
	float: left;
	padding: 0 10px;
}

.footer ul li:first-child {
	background: none;
}

.footerMenu {
	position: absolute;
	top: 7px;
	right: 10px;
	font-size: .85em;
}

.footerMenu ul {
	list-style: none;
}

.footerMenu ul li:first-child {
	background: none;
}

.footerMenu button {
	background: url(../images/bg.png) 0 -421px repeat-x;
	margin-top: -5px;
}

.footerMenu button:hover {
	background: url(../images/bg.png) 0 -292px repeat-x;
}

.tab {
	padding: 5px;
}

/*
 * Defines CSS styles associated with the navigation area of an application page.
 *
 */
.navigation { /*Cxx commented*/
	/*overflow: hidden;*/
	margin: 0px;
	padding: 0px 0px;
	clear: none;
	background-color: #efefef;
	width: 100%;
	/* IE Hack to force proper layout when preceded by a paragraph. (hasLayout Bug)*/
}
/* First Level Nav Styles */
.navigationMenu1 {
	margin: 0px;
	padding: 0px;
}

.navigationMenu1 ul li.mainMenuItem {
	position: relative;
	top: 1px;
	float: left;
	margin: 0px 0px 0px 0px;
	/*font-weight: bold;*/
	background-color: #efefef;
	list-style: none;
	border-left: none;
	border-bottom: solid 1px #98acbe;
	border-top: none;
	border-right: solid 1px #98acbe;
	-moz-user-select: none;
	-khtml-user-select: none;
	cursor: pointer;
	color: #666;
}

.navigationMenu1 a {
	display: block;
	padding: 5px 15px;
	border-bottom: 1px solid #efefef;
	color: #000;
	text-decoration: none;
	font: 1.5em "microsoft yahei";
}

.navigationMenu1 .verticalSubMenu {
	display: none;
	position: absolute;
	margin-top: 1px;
	z-index: 9999;
	width: 120px;
	overflow: hidden;
	font-size: 0.8em;
}

.navigationMenu1 .verticalSubMenu li {
	float: none;
	margin: 0px 0px 0px 0px;
	background-color: #fff;
	list-style: none;
	border-left: none;
	border-bottom: solid 1px #eee;
	border-top: none;
	border-right: none;
	-moz-user-select: none;
	-khtml-user-select: none;
	cursor: pointer;
}

.navigationMenu1 .verticalSubMenu li a {
	padding: 0px 0px 0px 5px;
	color: #999;
}

.navigationMenu1 a:hover,.navigationMenu1 a:focus {
	background-color: #e5eefa;
}

.navigationMenu1 .m-btn-downarrow {
	position: relative;
}

.navigationMenu1 .selected.mainMenuItem,.navigationMenu1 .selected.mainMenuItem a,.navigationMenu1 .selected.mainMenuItem a:hover,.navigationMenu1 .selected.mainMenuItem a:focus
	{
	background-color: #fefefe;
	border-bottom: 1px solid #fefefe;
	color: #000;
}
/* Second Level Nav Styles */
.navigationMenuContentGroup2 {
	border: 0;
	clear: both;
	border-top: solid 1px #98acbe;
	background-color: #fefefe;
}

.navigationMenuContent2 {
	overflow: hidden;
	background-color: #fbfafa;
	position: relative;
}

.navigationMenu2 {
	border-bottom: 1px solid #98acbe;
	background: url(../images/bg.png) repeat-x 0 -458px;
	padding: 0.2em 1em 0.4em;
	line-height: 1.5em;
	height: 1.7em;
}

.navigationMenu2 ul {
	list-style: none;
	padding: 0;
}

.navigationMenu2 ul li {
	font: 1em "microsoft yahei";
	float: left;
	display: inline-block;
	padding: 0 .5em 0 0;
}

.navigationMenu2 ul li:first-child {
	backgound: none;
}

.navigationMenu2 a,.navigationMenu2 a:visited {
	border-top: 3px solid transparent;
	text-decoration: none;
	color: #000000;
	display: block;
	text-align: center;
	cursor: pointer;
	padding: 0 1em .5em 1em;
}

.navigationMenu2 a img,.navigationMenu2 a:visited img {
	border: 0;
	vertical-align: middle;
}

.navigationMenu2 a:hover,.navigationMenu2 a:visited:hover,.navigationMenu2 a:focus,.navigationMenu2 a:visited:focus
	{
	border-top: 3px solid #ffcc00;
	text-decoration: none;
}

.navigationMenu2 .selected,.navigationMenu2 .selected a,.navigationMenu2 .selected a:hover,.navigationMenu2 .selected a:focus
	{
	border-top-color: #ff9900;
	font-weight: 400;
}
/* Third Level Nav Styles */
.navigationMenuContentGroup3 {
	border: 0;
	clear: both;
	border-left: solid 1px #acc3d7;
	border-bottom: solid 1px #acc3d7;
	border-top: solid 1px #98acbe;
	border-right: solid 1px #98acbe;
	background-color: #fefefe;
}

.navigationMenuContent3 {
	overflow: hidden;
	padding-top: 10px;
	/*min-height: 400px;*/
	background-color: #fbfafa;
	position: relative;
}

.navigationMenu3 {
	background: url(../images/bg.png) repeat-x 0 -458px;
	border-bottom: 1px solid #98acbe;
	margin: -10px -10px 0px -10px;
	padding: .4em 1em .6em 1em;
	line-height: 1.5em;
	height: 1.7em;
	font-size: .9em;
}

.navigationMenu3 ul {
	list-style: none;
	padding: 0;
}

.navigationMenu3 ul li {
	float: left;
	display: inline-block;
	padding: 0 .5em 0 0;
}

.navigationMenu3 ul li:first-child {
	backgound: none;
}

.navigationMenu3 a {
	border-top: 3px solid transparent;
	text-decoration: none;
	display: block;
	text-align: center;
	cursor: pointer;
	padding: 0 1em .5em 1em;
}

.navigationMenu3 a img {
	border: 0;
	vertical-align: middle;
}

.navigationMenu3 a:hover,.navigationMenu3 a:focus {
	border-top: 3px solid #ffcc00;
	text-decoration: none;
}

.navigationMenu3 .selected,.navigationMenu3 .selected a,.navigationMenu3 .selected a:hover,.navigationMenu3 .selected a:focus
	{
	border-top-color: #ff9900;
	font-weight: 400;
}

