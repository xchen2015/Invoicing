package com.pinfly.purchasecharge.core.model;

public class AccountingSearchForm extends SearchForm
{
    private static final long serialVersionUID = 1L;

    private long accountingTypeId;
    private AccountingModeCode accountingModeCode;

    public long getAccountingTypeId ()
    {
        return accountingTypeId;
    }

    public void setAccountingTypeId (long accountingTypeId)
    {
        this.accountingTypeId = accountingTypeId;
    }

    public AccountingModeCode getAccountingModeCode ()
    {
        return accountingModeCode;
    }

    public void setAccountingModeCode (AccountingModeCode accountingModeCode)
    {
        this.accountingModeCode = accountingModeCode;
    }
}
